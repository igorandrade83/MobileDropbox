<?xml version="1.0" encoding="UTF-8"?><widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="io.cronapp.apps.unifacsmobile" version="1.0.0">
    <name>UnifacsMobile</name>
    <description>UnifacsMobile</description>
    <author email="igor.andrade@techne.com.br" href="http://cronapp.io">Igor Andrade</author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <icon src="res/icon.png"/>
    <splash src="res/screen.png"/>
    <plugin name="cordova-plugin-cronapp" spec="2.1.1"/>
</widget>
